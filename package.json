{
  "name": "world-clock",
  "productName": "World Clock",
  "version": "0.0.1",
  "main": "electron_starter.js",
  "scripts": {
    "build:dev": "webpack",
    "build:prod": "webpack -p",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",    
    "start": "electron .",
    "test": "echo success"
  },
  "build": {
    "appId": "com.pawanhegde.electron-world-clock",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": ["**/*", "build/icon.png", "build/bundle.js"]
  },
  "private": false,
  "devDependencies": {
    "@babel/core": "7.0.0-beta.51",
    "@babel/preset-env": "7.0.0-beta.51",
    "@babel/preset-react": "7.0.0-beta.51",
    "babel-loader": "8.0.0-beta.3",
    "css-loader": "0.28.11",
    "electron": "^2.0.2",
    "electron-builder": "20.15.1",    
    "less": "3.0.4",
    "less-loader": "4.1.0",
    "style-loader": "0.21.0",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.3",
    "@material-ui/icons": "^1.1.0",
    "downshift": "^2.0.6",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.17",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-clock": "^2.3.0",
    "react-dom": "^16.4.1",
    "react-interval": "^2.0.2"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "automerge": true
  }
  "description": "Track time all over the world",
  "repository": "https://github.com/PawanHegde/electron-world-clock.git",
  "author": "Pawan Hegde <contact@pawanhegde.com>",
